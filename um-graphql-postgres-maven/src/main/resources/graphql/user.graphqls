type User {
    id: ID!
    firstName: String
    lastName: String
    password: String
    email: String
    active: Boolean
}

input UserInput {
    firstName: String
    lastName: String
    password: String
    email: String
}

input SearchFilterInput {
    field: StringFilterableField
    values: [String]!
}

enum StringFilterableField {
    FIRST_NAME
}